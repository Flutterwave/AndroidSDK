    <string name="mtn_validate_instructions">
       <![CDATA[
        <b>Please follow the instruction below to complete your MTN transaction</b>
        <br>
        <br>
        <p>1. Dial <b>*170#</b></p>
        <p>2. Choose option: <b>7) Wallet</b> </p>
        <p>3. Choose option: <b>3) My Approvals</b> </p>
        <p>4. Enter your MOMO pin to retrieve your pending approval list </p>
        <p>5. Choose a pending transaction </p>
        <p>6. Choose <b>option 1<b> to approve </p>
        <p>7. Tap button to continue </p>
        ]]>
    </string>

    <string name="tigo_validate_instructions">
        <![CDATA[
        <b>Please follow the instruction below to complete your Tigo transaction</b>
        <br>
        <br>
        <p>1. Dial <b>5015#</b> to approve your transaction.</p>
        <p>2. Select the transaction to approve and click on send.</p>
        <p>3. Select YES to confirm your payment.</p>
        ]]>
    </string>

